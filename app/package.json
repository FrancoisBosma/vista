{
  "name": "rapide-et-furieux",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=production vite-ssg build",
    "dev": "run-p dev:*",
    "dev:core": "vite --port 3333",
    "dev:gql-watch": "pnpm gql-codegen -- --watch",
    "gql-codegen": "graphql-codegen --config src/api/gql-codegen.ts",
    "lint": "run-s lint:*",
    "lint:css": "stylelint --fix \"**/*.{css,scss,vue}\"",
    "lint:i18n": "pnpm exec vue-i18n-extract && pnpm exec vue-i18n-extract --remove",
    "lint:imports": "import-sort --write \"src/**/*{.js,.ts}\"",
    "lint:js": "eslint --fix --ext \"**/*.{js,ts,vue}\"",
    "lint:prettier": "prettier --write --loglevel error \"**/*.{js,ts,json,css,scss,vue,html,md}\"",
    "lint:types": "vue-tsc --noEmit",
    "preinstall": "npx only-allow pnpm",
    "prepare": "cd .. && husky install && chmod ug+x .husky/*",
    "preview": "vite preview",
    "preview-https": "serve dist",
    "sec-audit": "run-s sec-audit:*",
    "sec-audit:dev": "pnpm audit --json --dev",
    "sec-audit:force:fix": "pnpm audit --fix",
    "sec-audit:prod": "pnpm audit --json --prod",
    "test:ci": "run-s test:ci:*",
    "test:ci:unit": "vitest",
    "test:ci:e2e": "cypress run --headless",
    "test:docker": "docker run -it --rm -v $(pwd):/opt/my_app my_app_dev:latest $1",
    "test:docker:unit": "pnpm test:docker test:local:unit",
    "test:docker:e2e": "pnpm test:docker test:local:e2e",
    "test:local": "run-s test:local:*",
    "test:local:unit": "vitest",
    "test:local:e2e": "cypress open"
  },
  "dependencies": {
    "@vueuse/core": "^9.13.0",
    "@vueuse/gesture": "2.0.0-beta.1",
    "@vueuse/head": "^1.1.23",
    "@vueuse/integrations": "^9.13.0",
    "axios": "^1.3.4",
    "graphql": "^16.6.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.33",
    "unfetch": "^5.0.0",
    "villus": "^3.0.0",
    "vue": "^3.2.47",
    "vue-demi": "^0.13.11",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.37.0",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@cypress/vite-dev-server": "^5.0.5",
    "@cypress/vue": "^5.0.5",
    "@graphql-codegen/cli": "3.2.2",
    "@graphql-codegen/client-preset": "2.1.1",
    "@graphql-codegen/introspection": "3.0.1",
    "@iconify-json/carbon": "^1.1.16",
    "@intlify/unplugin-vue-i18n": "^0.9.3",
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/compiler-sfc": "^3.2.47",
    "@vue/server-renderer": "^3.2.47",
    "@vue/test-utils": "^2.3.1",
    "critters": "^0.0.16",
    "cross-env": "^7.0.3",
    "cypress": "^12.8.1",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.9.0",
    "https-localhost": "^4.7.1",
    "husky": "^8.0.3",
    "import-sort": "^6.0.0",
    "import-sort-cli": "^6.0.0",
    "import-sort-config": "^6.0.0",
    "import-sort-parser-babylon": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-eslint": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "pnpm": "^7.30.0",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.5",
    "prettier-plugin-import-sort": "^0.0.7",
    "stylelint": "^15.3.0",
    "stylelint-config-css-modules": "^4.2.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard-scss": "^7.0.1",
    "typescript": "^5.0.2",
    "unplugin-auto-import": "^0.15.1",
    "unplugin-icons": "^0.15.3",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.2.1",
    "vite-plugin-inspect": "^0.7.18",
    "vite-plugin-pages": "^0.29.0",
    "vite-plugin-pwa": "^0.14.4",
    "vite-plugin-vue-layouts": "^0.8.0",
    "vite-plugin-windicss": "^1.8.10",
    "vite-ssg": "^0.22.1",
    "vitest": "^0.29.5",
    "vue-i18n-extract": "^2.0.7",
    "vue-tsc": "^1.2.0"
  },
  "engines": {
    "node": ">=16.20.0"
  }
}
